{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    "react/no-unused-prop-types": [1, { "skipShapeProps": true }]
  }
}